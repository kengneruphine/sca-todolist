{
  "name": "sca-todolist",
  "version": "1.0.0",
  "description": "Final project for the She Code Africa Mentoring program",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node -r esm src/server.js",
    "test": "mocha",
    "cover": "node_modules/istanbul/lib/cli.js cover node_modules/mocha/bin/_mocha -- -R spec test/*",
    "dev": "NODE_ENV=development npm-run-all build src",
    "prod": "NODE_ENV=production npm-run-all build src",
    "transpile": "babel ./src --out-dir dist-src",
    "clean": "rimraf dist-src",
    "build": "npm-run-all clean transpile"
  },
  "author": "Ruphine kengne",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.19.0",
    "chai": "^4.2.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-prettier": "^3.1.2",
    "esm": "^3.2.25",
    "express": "^4.17.3",
    "mocha": "^7.0.1",
    "nodemon": "^2.0.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.4",
    "@babel/preset-env": "^7.8.4",
    "babel-preset-es2015": "^6.24.1",
    "coveralls": "^3.0.9",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.20.1",
    "istanbul": "^0.4.5"
  }
}
